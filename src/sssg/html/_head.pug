head(prefix="og: http://ogp.me/ns# article: http://ogp.me/ns/article#")
  meta(charset="utf-8")
  meta(name="viewport", content="initial-scale=1.0")
  title= site_title
  meta(name="author", content="Hinaser")
  meta(name="generator", content="SSSG - https://www.npmjs.com/package/sssg")
  meta(name="articleID", content=article.id)
  meta(name="keywords", content=article.keywords)
  if(article.description)
    meta(name="description", content=article.description)

  meta(property="og:type", content="article")
  meta(property="og:title", content=article.title)
  if(article.image)
    meta(name="og:image", content=article.image)

  meta(property="article:section", content=article.topic)
  meta(property="article:tag", content=article.subtopic)
  meta(property="article:published_time", content=article.create_dtime)

  meta(name='twitter:card', content='summary')
  meta(name='twitter:title', content=article.title)
  if(article.description)
    meta(name='twitter:description', content=article.description)
  if(article.image)
    meta(name='twitter:image', content=article.image)

  link(rel="stylesheet", href="//fonts.googleapis.com/earlyaccess/notosansjapanese.css")
  link(rel="stylesheet", href="//fonts.googleapis.com/css?family=Asap:4000,700")
  link(href=url("css/lib.css"), rel="stylesheet")
  // uncache
  link(href=url("css/main.css"), rel="stylesheet")
  // enduncache
  script(type="text/javascript", src="//platform-api.sharethis.com/js/sharethis.js#property=59ec9ef395f7b200121c2a3d&product=inline-share-buttons")
  script(src=url("js/lib.js"))
  script(src=url("misc/article_list.js"))
  // uncache
  script(src=url("js/main.js"))
  // enduncache
